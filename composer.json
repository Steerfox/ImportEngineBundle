{
  "name": "steerfox/import-engine-bundle",
  "type": "symfony-bundle",
  "description": "A generic import (and export) -engine that provides easy to use yet powerful features",
  "keywords": [
    "csv",
    "import"
  ],
  "homepage": "https://github.com/Steerfox/ImportEngineBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Markus Thielen",
      "email": "markus@logicx.de"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "steerfox/import-engine": ">=0.8.6",
    "symfony/framework-bundle": "~2.4|~3.0"
  },
  "require-dev": {
    "symfony/console": "~2.8|~3.0",
    "symfony/validator": "~2.8|~3.0",
    "phpunit/phpunit": "*",
    "sensio/generator-bundle": "~2.3",
    "jms/serializer": "~1.4",
    "doctrine/orm": "~2.5"
  },
  "suggest": {
    "symfony/console": "If you want to use the Commands",
    "sensio/generator-bundle": "If you want to use the GenerateImportValueObjectCommand"
  },
  "autoload": {
    "psr-0": {
      "Mathielen\\ImportEngineBundle": ""
    }
  },
  "target-dir": "Mathielen/ImportEngineBundle"
}
